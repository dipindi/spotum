{
    "manifest_version": 3,
    "name": "Spotum AI",
    "version": "1.0",
    "description": "Upload a file to Spotum AI for prediction",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
  